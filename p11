#include <stdio.h>
#include <stdlib.h>

int a[20][20], n, visited[20];

void bfs(int start)
{
    int q[20], front = 0, rear = 0, v, i;
    visited[start] = 1;
    q[rear++] = start;

    while (front < rear)
    {
        v = q[front++];
        printf("%d ", v);

        for (i = 1; i <= n; i++)
            if (a[v][i] && !visited[i])
            {
                visited[i] = 1;
                q[rear++] = i;
            }
    }
}

void dfs(int v)
{
    int i;
    visited[v] = 1;
    printf("%d ", v);

    for (i = 1; i <= n; i++)
        if (a[v][i] && !visited[i])
            dfs(i);
}

int main()
{
    int i, j, start, ch;

    printf("Enter number of vertices: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix:\n");
    for (i = 1; i <= n; i++)
        for (j = 1; j <= n; j++)
            scanf("%d", &a[i][j]);

    printf("Enter starting vertex: ");
    scanf("%d", &start);

    printf("1.BFS  2.DFS\n");
    scanf("%d", &ch);

    for (i = 1; i <= n; i++) visited[i] = 0;

    if (ch == 1)
        bfs(start);
    else if (ch == 2)
        dfs(start);
    else
        printf("Invalid choice");

    return 0;
}
